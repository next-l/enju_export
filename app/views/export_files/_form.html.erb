<%= simple_form_for(@export_file) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :export_file_name %>
    <%= f.input :export_content_type %>
    <%= f.input :export_file_size %>
    <%= f.input :state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
